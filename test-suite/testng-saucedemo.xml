<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Saucedemo ecommerce playground website test suite" thread-count="4"
       parallel="tests" verbose="5">

    <listeners>
        <listener
              class-name="org.saucedemo.listeners.ExtentReportListener" />

    </listeners>

    <test name="Web Automation Demo tests with Selenium">
        <parameter name="browser" value="chrome" />
    <classes>
        <class name="org.saudemo.test.LoginTest"/>
    </classes>
</test>
</suite> <!-- Suite -->